{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/health$", "dest": "/api/health.js" },
    { "src": "^/api/health$", "dest": "/api/health.js" },
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}
